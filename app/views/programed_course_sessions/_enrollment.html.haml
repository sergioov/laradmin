= simple_form_for [@programed_course, @programed_course.enrollments.build] do |f|
  = f.error_notification
  .form-inputs
    = f.input :student, label:"Alumno (Buscar por apellido)", :url => autocomplete_person_last_name_programed_course_sessions_path, as: :autocomplete, input_html: { class: 'form-input form-control' }, :id_element => '#student_id'
    =f.hidden_field :student_id, :id => "student_id"

  .form-actions
    = f.button :submit, "Inscribir", class: "form-button btn btn-default"

